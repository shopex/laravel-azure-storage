{
    "name": "onex/laravel-azure-storage",
    "description": "Microsoft Azure Blob Storage integration for Laravel's Storage API",
    "type": "library",
    "keywords": ["storage","laravel","azure"],
    "require": {
        "league/flysystem-azure-blob-storage": "^0.1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "psy/psysh": "^0.8.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Onex\\LaravelAzureStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onex\\LaravelAzureStorage\\AzureStorageServiceProvider"
            ]
        }
    }
}
